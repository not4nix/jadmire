<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.jadmire.core.user.domain.User" table="tbl_users" schema="" catalog="jadmire">
        <id name="id">
            <column name="id" sql-type="int" length="10" not-null="true"/>
        </id>
        <property name="userName">
            <column name="user_name" sql-type="varchar" length="15" not-null="true"/>
        </property>
        <property name="userEmail">
            <column name="user_email" sql-type="varchar" length="15" not-null="true"/>
        </property>
        <property name="userPassword">
            <column name="user_password" sql-type="varchar" length="15" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>